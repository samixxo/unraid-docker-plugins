<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
    <Name>tinyproxy</Name>
    <Project>https://github.com/tinyproxy/tinyproxy</Project>
    <Icon>https://raw.githubusercontent.com/samixxo/unraid-docker-plugins/master/plugin/icon/tinyproxy.png</Icon>
    <Overview>Tinyproxy is a small, efficient HTTP/SSL proxy daemon released under the GNU General Public License.</Overview>
    <GitHub>https://github.com/monokal/docker-tinyproxy</GitHub>
    <Registry>https://hub.docker.com/r/monokal/tinyproxy</Registry>
    <Repository>monokal/tinyproxy</Repository>
    <Privileged>false</Privileged>
    <BindTime>true</BindTime>
    <Network Default="bridge">bridge</Network>
    <Config Required="true" Type="Port" Name="Proxy Port"
            Target="8888" Default="17080" Mode="tcp" Display="always-hide"
            Description="Port used to proxy incoming request"/>
    <PostArgs>
        ANY
    </PostArgs>
</Container>